  3.842988376120022*^9}},ExpressionUUID->"45814b51-8a74-4516-89e3-\
04e0c32dac04"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"collectLog", "[", "expr_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"rule1", ",", "rule2", ",", "a", ",", "b", ",", "x"}], "}"}], 
     ",", 
     RowBox[{
      RowBox[{"rule1", "=", 
       RowBox[{
        RowBox[{
         RowBox[{"Log", "[", "a_", "]"}], "+", 
         RowBox[{"Log", "[", "b_", "]"}]}], "\[Rule]", 
        RowBox[{"Log", "[", 
         RowBox[{"a", "*", "b"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"rule2", "=", 
       RowBox[{
        RowBox[{"x_", "*", 
         RowBox[{"Log", "[", "a_", "]"}]}], "\[Rule]", 
        RowBox[{"Log", "[", 
         RowBox[{"a", "^", "x"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"expr", "/.", "rule1"}], ")"}], "/.", "rule2"}], "/.", 
        "rule1"}], "/.", "rule2"}]}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"expandLog", "[", "expr_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"rule1", ",", "rule2", ",", "a", ",", "b", ",", "x"}], "}"}], 
     ",", 
     RowBox[{
      RowBox[{"rule1", "=", 
       RowBox[{
        RowBox[{"Log", "[", 
         RowBox[{"a_", "*", "b_"}], "]"}], "\[Rule]", 
        RowBox[{
         RowBox[{"Log", "[", "a", "]"}], "+", 
         RowBox[{"Log", "[", "b", "]"}]}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"rule2", "=", 
       RowBox[{
        RowBox[{"Log", "[", 
         RowBox[{"a_", "^", "x_"}], "]"}], "\[Rule]", 
        RowBox[{"x", "*", 
         RowBox[{"Log", "[", "a", "]"}]}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"expr", "/.", "rule1"}], ")"}], "/.", "rule2"}]}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"expandAllLog", "[", "expr_", "]"}], ":=", 
  RowBox[{"Nest", "[", 
   RowBox[{"expandLog", ",", "expr", ",", 
    RowBox[{"Depth", "[", "expr", "]"}]}], "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"collectAllLog", "[", "expr_", "]"}], ":=", 
   RowBox[{"Nest", "[", 
    RowBox[{"collectLog", ",", "expr", ",", 
     RowBox[{"Length", "[", "expr", "]"}]}], "]"}]}], ";"}], "\n"}], "Input",
